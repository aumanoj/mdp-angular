{
  "name": "login-register-jwt",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve",
    "start:fr": "ng serve --configuration=fr-FR",
    "start:en": "ng serve --configuration=en-US",
    "start": "node server.js",
    "build": "ng build",
    "build:fr": "ng build --configuration=fr-FR",
    "build:en": "ng build --configuration=en-US",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prod": "ng build --prod --base-href=\"./\"",
    "publish.in.heroku": "npm run prod && git add . && git commit -m \"Publish in heroku\" && git push heroku master",
    "i18n:extract": "ng xi18n --output-path src/locales",
    "build-i18n:fr": "ng build --output-path=dist/fr --aot --prod --base-href /fr/ --i18n-file=src/locales/french/messages.fr.xlf --i18n-format=xlf --i18n-locale=fr",
    "build-i18n:en": "ng build --output-path=dist/ --aot --prod --base-href / --i18n-file=src/locales/english/messages.en.xlf --i18n-format=xlf --i18n-locale=en",
    "build-i18n": "npm run build-i18n:en && npm run build-i18n:fr"
  },
  "private": true,
